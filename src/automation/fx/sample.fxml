<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" prefHeight="400.0" prefWidth="600.0" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="automation.fx.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0">
      <Label alignment="CENTER" prefWidth="580.0" style="-fx-background-color: #feb236;" text="Garage Door" textAlignment="CENTER">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" prefWidth="600.0">
         <Button fx:id="buttonGarageDoor" mnemonicParsing="false" prefWidth="245.0" style="-fx-background-color: red;" text="OFF">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </Button>
         <Label text="Percent:">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </Label>
         <ComboBox fx:id="comboBoxGarageDoor" prefWidth="245.0" promptText="75">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </ComboBox>
      </HBox>
      <Label alignment="CENTER" prefWidth="580.0" style="-fx-background-color: #feb236;" text="Living Room Window">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="43.0" prefWidth="600.0">
         <Button fx:id="buttonLivingRoomWindow" mnemonicParsing="false" prefWidth="245.0" style="-fx-background-color: red;" text="OFF">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </Button>
         <Label text="Percent:">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </Label>
         <ComboBox fx:id="comboBoxLivingRoomWindow" prefWidth="245.0" promptText="100">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </ComboBox>
      </HBox>
      <Label alignment="CENTER" layoutX="20.0" layoutY="83.0" prefWidth="580.0" style="-fx-background-color: #feb236;" text="Bathroom Heating">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="105.0" prefWidth="600.0">
         <Button fx:id="buttonBathRoomHeating" mnemonicParsing="false" prefWidth="245.0" style="-fx-background-color: red;" text="OFF">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </Button>
         <Label text="Temperature:">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </Label>
         <TextField fx:id="textFieldBathroomHeating" prefWidth="220.0" text="20">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </TextField>
      </HBox>
      <Label alignment="CENTER" layoutX="20.0" layoutY="145.0" prefWidth="580.0" style="-fx-background-color: #feb236;" text="Kitchen Oven">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="168.0" prefWidth="600.0">
         <Button fx:id="buttonKitchenOven" mnemonicParsing="false" prefWidth="245.0" style="-fx-background-color: red;" text="OFF">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </Button>
         <Label text="Temperature:">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </Label>
         <TextField fx:id="textFieldKitchenOven" prefWidth="220.0" text="180">
            <HBox.margin>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
         </TextField>
      </HBox>
      <HBox prefWidth="200.0">
         <VBox alignment="TOP_CENTER" prefWidth="300.0">
            <Label alignment="CENTER" prefWidth="280.0" style="-fx-background-color: #feb236;" text="Bathroom Light">
               <VBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="buttonBathRoomLight" mnemonicParsing="false" prefWidth="245.0" style="-fx-background-color: red;" text="OFF">
               <VBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Button>
         </VBox>
         <VBox alignment="TOP_CENTER" layoutX="10.0" layoutY="10.0" prefWidth="300.0">
            <Label alignment="CENTER" prefWidth="280.0" style="-fx-background-color: #feb236;" text="Living Room Light">
               <VBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="buttonLivingRoomLight" mnemonicParsing="false" prefWidth="245.0" style="-fx-background-color: red;" text="OFF">
               <VBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </VBox.margin>
            </Button>
         </VBox>
      </HBox>
      <Button fx:id="buttonApplyChanges" mnemonicParsing="false" onAction="#saveState" text="Apply Changes">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="40.0" />
         </VBox.margin>
      </Button>
   </VBox>
</GridPane>
